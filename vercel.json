{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/leaderboard",
      "destination": "/index.html"
    },
    {
      "source": "/achievements",
      "destination": "/index.html"
    },
    {
      "source": "/logros",
      "destination": "/index.html"
    },
    {
      "source": "/rewards",
      "destination": "/index.html"
    },
    {
      "source": "/ranking",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}